<% @users.each do |u|%>
<%= u.email %>
  <%= form_with model: @request do |form| %>
    <%= form.hidden_field :invitor_id, value: current_user.id%>
    <%= form.hidden_field :invitee_id, value: u.id %>
    <%= form.hidden_field :accepted, value: false %>
    <%= form.submit "Request Friend"%>
  <% end %>
<% end %>
